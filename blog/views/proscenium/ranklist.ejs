                                    <th style="text-align: center">文章数量</th>
                                    <th style="text-align: right">浏览量</th>
                                </tr>
                                </thead>
                                <tbody>
                                <% for(var i = 0; i < 10; i++) { %>
                                <tr>
                                    <td style="text-align: center"><%= i %></td>
                                    <td style="text-align: center">那一天未闻花名</td>
                                    <td style="text-align: center">12</td>
                                    <td style="text-align: right">13343333</td>
                                </tr>
                                <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

